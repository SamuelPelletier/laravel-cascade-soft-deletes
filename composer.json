{
  "name": "pellesam/laravel-cascade-soft-delete-and-restore",
  "description": "Cascading delete and restore for Eloquent models that implement soft delete",
  "type": "utility",
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/database": "^8.12|^9.0",
    "illuminate/events": "^8.12|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Pellesam\\Database\\Support\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Samuel Pelletier",
      "email": "pellesam.info@gmail.com",
      "homepage": "https://samuelpelletier.fr"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "extra": {
      "sort-packages": true
    }
  }
}
